@using Hyt.Model.WorkflowStatus
@model Hyt.Model.UnUnionSite
@{
    ViewBag.Title = "联盟网站查看";
}
<div class="caseout">
    <div class="boxs_detail_dispay" id="toLook">
        <div class="tit_con">
            <table width="100%" cellspacing="0" cellpadding="0" border="0">
                <tr>
                    <td class="nameright border_rnone" style="width: 20%">网站名称：</td>
                    <td colspan="3">
                       @Model.SiteName
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">网站URL：</td>
                    <td  colspan="3">
                       @Model.SiteUrl
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">联系人：</td>
                    <td  colspan="3">
                        @Model.Contact
                    </td>
                   
                </tr>
                <tr>
                     <td class="nameright border_rnone" >电话：</td>
                    <td colspan="3">
                        @Model.Telephone
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">手机：</td>
                    <td  colspan="3">
                       @Model.MobilePhoneNumber
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">有效日期起：</td>
                    <td  colspan="3">
                        @Model.BeginTime.ToString("yyyy-MM-dd")
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">有效日期止：</td>
                    <td  colspan="3">
                        @Model.EndTime.ToString("yyyy-MM-dd")
                    </td>
                </tr>
                <tr>
                    <td class="nameright border_rnone">状态：</td>
                    <td  colspan="3">
                        @((UnionStatus.联盟网站状态)Model.Status)
                    </td>
                </tr>
            </table>

        </div>
    </div>
</div>
