@using Hyt.Model.SystemPredefined
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "兴业嘉同步日志详情";
    @Html.PrivilegeControl(PrivilegeCode.EAS1001101, ".XingYe1001101")
}
<script type="text/javascript" src="/Theme/scripts/Utils.yui.js"></script>
<script type="text/javascript" src="/Theme/Scripts/jquery.unobtrusive-ajax.js"></script>
<div class="case">
    <div class="boxs">
        <div class="boxs_tit head_m">
            <h3>系统编号:@ViewBag.Key</h3>
        </div>

        <div class="boxs_con_c1">
            <!--表格列开始-->
            <div id="paging"></div>
        </div>

        <script type="text/javascript">
            $(document).ready(function () {
                doSearch();
            });

            //查询列表
            function doSearch() {
                Ajax.get("/Sys/DoXingYeQuery", "", function (p) {
                    p.setParams("sysNo",@{@ViewBag.Key});
                    }, function (data) {
                        $('#paging').empty();
                        $('#paging').html(data);
                    });
                    }
        </script>
    </div>

</div>
