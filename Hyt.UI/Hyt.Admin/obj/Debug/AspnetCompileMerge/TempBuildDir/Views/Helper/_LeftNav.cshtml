<script type="text/javascript">
var objCom=$('.py_fl_box');
var objClick=objCom.find('dl > dt');
objClick.toggle(
	function(){
		$(this).children('span').removeClass('i_close').addClass('i_open');
		$(this).next('dd').show();
	},
	function(){
		$(this).children('span').removeClass('i_open').addClass('i_close');
		$(this).next('dd').hide();
	}
);
</script>
